{
  "name": "digiency-landing",
  "version": "1.0.0",
  "description": "Landing page",
  "main": "index.js",
  "scripts": {
    "sass": "sass scss/styles.scss css/styles.css",
    "scss:autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers \"last 2 versions\" -r css/styles.css",
    "watch": "sass --watch scss/styles.scss:css/styles.css",
    "build": "npm run sass && npm run scss:autoprefixer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadiminsk/digiency-landing.git"
  },
  "author": "Vadim Gadjimuradov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vadiminsk/digiency-landing/issues"
  },
  "homepage": "https://github.com/vadiminsk/digiency-landing#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.3",
    "node-sass": "^6.0.1",
    "postcss-cli": "^8.3.1"
  }
}
